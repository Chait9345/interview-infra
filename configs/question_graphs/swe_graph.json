      "difficulty": "easy",
      "prompt_id": "array_traversal_logic",

      "success_threshold": 8,
      "partial_threshold": 5,
      "failure_threshold": 0,

      "transitions": {
        "probe_deeper": {
          "if": "score >= 8 AND reasoning_clear == true",
          "then": "PROBE_SAME_NODE_DEEPER"
        },
        "downgrade_difficulty": {
          "if": "silence_seconds > 10 OR guessing_without_reasoning == true",
          "then": "dsa_array_1"
        },
        "move_forward": {
          "if": "score >= 5 AND score < 8",
          "then": "dsa_hashing_1"
        }
      },

      "edge_cases": {
        "silence_over_10s": "downgrade_difficulty",
        "guessing_without_reasoning": "downgrade_difficulty",
        "incomplete_answer": "move_forward"
      },

      "governance": {
        "allow_probe": true,
        "max_probe_depth": 1,
        "counts_toward_difficulty_exposure": true,
        "forbidden_transition_reasons": [
          "confidence_estimation",
          "emotion_inference",
          "speech_fluency"
